USE ROLE accountadmin;
CREATE SHARE SNOWFLAKE_CAPSTONE;
GRANT USAGE ON DATABASE DW_IPL TO SHARE SNOWFLAKE_CAPSTONE;
GRANT USAGE ON SCHEMA IPL_ETL TO SHARE SNOWFLAKE_CAPSTONE;
GRANT SELECT ON ALL TABLES IN SCHEMA IPL_ETL TO SHARE SNOWFLAKE_CAPSTONE;
SHOW GRANTS TO SHARE SNOWFLAKE_CAPSTONE;
ALTER SHARE SNOWFLAKE_CAPSTONE ADD ACCOUNTS=UBDHOZH.QS53720
SHOW GRANTS OF SHARE SNOWFLAKE_CAPSTONE;